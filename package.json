{
  "name": "diddyback",
  "version": "0.1.0",
  "description": "diddytracker backend",
  "author": "Kalyan Nath",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "build": "tsup src/index.ts --format cjs --target node18 --outDir dist",
    "test": "vitest",
    "generate": "prisma generate",
    "scrape": "tsx prisma/scrape-supreme.ts",
    "parse:local": "tsx prisma/parse-local.ts",
    "db:backfill-stockx": "tsx prisma/backfill-stockx-ids.ts",
    "get:stockx:tokens": "node scripts/get-stockx-tokens.js",
    "test:stockx-sync": "node scripts/test-sync.js",
    "migrate": "prisma migrate dev",
    "db:push": "prisma db push",
    "db:seed": "ts-node prisma/seed.ts",
    "lint": "echo 'add lint script if needed'"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@prisma/client": "^5.22.0",
    "axios": "^1.10.0",
    "cheerio": "^1.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.6.1",
    "express": "^4.19.2",
    "node-cron": "^3.0.3",
    "puppeteer": "^24.14.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "supcommunity-api": "^1.0.5",
    "supreme-community-api": "^4.0.1",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^22.10.2",
    "nodemon": "^3.0.1",
    "prisma": "5.22.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "tsup": "^8.0.2",
    "tsx": "^4.11.0"
  }
}